    "import glob\n",
    "\n",
    "data_folder = glob.glob(\"../Read_Up/*\")\n",
    "print(data_folder)\n",
    "\n",
    "for session in data_folder:\n",
    "    for aud_file in glob.glob(\"%s/[0-9].*\"%session):\n",
    "        os.rename(aud_file, aud_file[:-5]+'0'+aud_file[-5:])\n",
    "        print(aud_file[-6:])"
